<h2>Porfolio Items</h2>
<p id="notice"><%= notice %></p>
<% @portfolio_items.each do |porfolio_item| %>
<p><%= porfolio_item.title %></p>
<p><%= porfolio_item.subtitle %></p>
<p><%= porfolio_item.body %></p>
<p><%= image_tag porfolio_item.thumb_image unless porfolio_item.thumb_image.nil? %></p>

<% end -%>